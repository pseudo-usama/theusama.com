{
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/assets/usama-cv.pdf",
        "destination": "/usama-resume.pdf",
        "type": 301
      },
      {
        "source": "/usama-cv.pdf",
        "destination": "/usama-resume.pdf",
        "type": 301
      },
      {
        "source": "/cv.pdf",
        "destination": "/usama-resume.pdf",
        "type": 301
      },
      {
        "source": "/resume.pdf",
        "destination": "/usama-resume.pdf",
        "type": 301
      },
      {
        "source": "/usama-resume",
        "destination": "/usama-resume.pdf",
        "type": 301
      },
      {
        "source": "/usama-cv",
        "destination": "/usama-resume.pdf",
        "type": 301
      },
      {
        "source": "/cv",
        "destination": "/usama-resume.pdf",
        "type": 301
      },
      {
        "source": "/resume",
        "destination": "/usama-resume.pdf",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/usama-resume.pdf",
        "destination": "/assets/usama-cv.pdf"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}